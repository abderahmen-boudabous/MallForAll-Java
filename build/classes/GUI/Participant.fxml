<AnchorPane id="window" prefHeight="650.0" prefWidth="1144.0" style="-fx-background-color: #34568B;" stylesheets="@../image/window.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ParticipantController">
    <children>
        <VBox layoutX="155.0" layoutY="255.0" prefHeight="342.0" prefWidth="828.0" spacing="10">
            <HBox prefHeight="116.0" prefWidth="697.0" spacing="10">
                <Label id="label" prefHeight="21.0" prefWidth="142.0" stylesheets="@../image/window.css" text="Event:" />
                <ComboBox id="sale" fx:id="eventComboBox" prefHeight="59.0" prefWidth="413.0" stylesheets="@../image/stylesheet.css" />
            </HBox>
            <HBox prefHeight="69.0" prefWidth="564.0" spacing="10">
                <Label id="label" prefHeight="27.0" prefWidth="185.0" stylesheets="@../image/window.css" text="Shop Name:" />
                <TextField id="text" fx:id="sellerNameField" prefHeight="45.0" prefWidth="436.0" stylesheets="@../image/window.css" />
               <HBox alignment="center" prefHeight="69.0" prefWidth="287.0" spacing="10" />
            </HBox>
             <Button id="sale" onAction="#addParticipant" prefHeight="52.0" prefWidth="838.0" stylesheets="@../image/stylesheet.css" text="Participate" textAlignment="CENTER" />
        </VBox>

      <Button id="sale" fx:id="chartButton" layoutX="748.0" layoutY="55.0" mnemonicParsing="false" onAction="#handleChartButton" prefHeight="59.0" prefWidth="235.0" stylesheets="@../image/stylesheet.css" text="Show Chart" />
      <Label id="label" layoutX="155.0" layoutY="55.0" prefHeight="59.0" prefWidth="608.0" stylesheets="@../image/window.css" text="You may want to take a look at our chart before participating" textAlignment="CENTER" textFill="WHITE" textOverrun="LEADING_WORD_ELLIPSIS">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      
      

    </children>
   <cursor>
      <Cursor fx:constant="MOVE" />
   </cursor>
   <effect>
      <ColorAdjust />
   </effect>
</AnchorPane>
