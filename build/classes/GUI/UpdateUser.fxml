<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.UpdateuserController">
    <children>
        <TextField fx:id="mail" layoutX="50.0" layoutY="50.0" promptText="Email" style="-fx-border-width: 0px 0px 2px 0px; -fx-border-color: black; -fx-prompt-text-fill: black; -fx-background-color: transparent;" />
        <TextField fx:id="nom" layoutX="50.0" layoutY="100.0" promptText="Nom" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px; -fx-prompt-text-fill: #000000;" />
        <TextField fx:id="Adreese" layoutX="50.0" layoutY="150.0" promptText="Adresse" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px; -fx-prompt-text-fill: #000000;" />
        <TextField fx:id="prenom" layoutX="50.0" layoutY="200.0" promptText="Prenom" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px; -fx-prompt-text-fill: #000000;" />
        <TextField fx:id="codepostale" layoutX="50.0" layoutY="250.0" promptText="Code Postale" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px; -fx-prompt-text-fill: #000000;" />
        <TextField fx:id="ville" layoutX="50.0" layoutY="300.0" promptText="Ville" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px; -fx-prompt-text-fill: #000000;" />
        <TextField fx:id="nomBoutique" layoutX="50.0" layoutY="350.0" promptText="Nom de la Boutique" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px; -fx-prompt-text-fill: #000000;" />
        <Button fx:id="updateprofile" layoutX="50.0" layoutY="400.0" mnemonicParsing="false" onAction="#modifier" text="Modifier" />
    </children>
</AnchorPane>
