<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="gridpane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="gui.FXML1Controller">
    <columnConstraints>
        <ColumnConstraints percentWidth="20" />
        <ColumnConstraints percentWidth="20" />
        <ColumnConstraints percentWidth="20" />
        <ColumnConstraints percentWidth="20" />
        <ColumnConstraints percentWidth="20" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Label text="Photo" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <Label text="Name" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <Label text="Type" GridPane.columnIndex="2" GridPane.rowIndex="0" />
        <Label text="Price" GridPane.columnIndex="3" GridPane.rowIndex="0" />
        <Label text="Stock" GridPane.columnIndex="4" GridPane.rowIndex="0" />
        <ImageView fx:id="photo" GridPane.columnIndex="0" GridPane.rowIndex="1" fitWidth="100" fitHeight="100" />
        <Label fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label fx:id="type" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Label fx:id="price" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Label fx:id="stock" GridPane.columnIndex="4" GridPane.rowIndex="1" />
    </children>
</GridPane>
